<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fandm.saad.accountya">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Accountya">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3897852847996080~2858056189"/>

        <activity android:name=".ShowSolutionActivity"
            android:parentActivityName=".FormulaSolverActivity" />
        <activity
            android:name=".FormulaSolverActivity"
            android:hardwareAccelerated="true"
            android:parentActivityName=".BaseFormulaActivity" />
        <activity
            android:name=".BaseFormulaActivity"
            android:parentActivityName=".HomeActivity" />

        <activity android:name=".HomeActivity"
            android:hardwareAccelerated="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>